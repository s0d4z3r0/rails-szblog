<% content_for :navbar do %>
    <%= render "shared/navbar" %>
<% end %>

<h1>Login</h1>

<%= form_with url: login_path, local: true do |form| %>
    <div>
        <%= form.label :email %><br>
        <%= form.email_field :email %>
    </div>
    <div>
        <%= form.label :password %><br>
        <%= form.password_field :password %>
    </div>
    <div>
        <%= form.submit "Entrar" %>
    </div>
<% end %>
